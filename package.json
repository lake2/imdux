        "is-primitive": "^3.0.1",
        "redux": "^4.0.5"
    },
    "devDependencies": {
        "@rollup/plugin-typescript": "^8.1.0",
        "@types/jest": "^25.2.3",
        "coveralls": "^3.1.0",
        "jest": "^25.5.4",
        "prettier": "^2.2.1",
        "rollup": "^2.35.1",
        "typescript": "^3.9.7"
