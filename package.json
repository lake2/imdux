{
    "name": "imdux",
    "version": "0.3.0",
    "description": "A redux helper for react & hooks & typescript developers.",
    "main": "dist/imdux.js",
    "module": "es/imdux.js",
    "types": "src/types.d.ts",
    "scripts": {
        "test": "jest",
        "cover": "jest --coverage --coverageReporters=text-lcov | coveralls",
        "dev": "webpack-dev-server --config webpack.dev.config",
